<workspace name="dsp_lib_fft">
  <project device="EFM32GG11B820F2048GL192"
           name="EFM32GG11B_dsp_lib_fft">
    <targets>
      <name>slsproj</name>
    </targets>
    <directories>
      <cmsis>$PROJ_DIR$\..\..\..\..\..\platform\CMSIS</cmsis>
      <device>$PROJ_DIR$\..\..\..\..\..\platform\Device\SiliconLabs</device>
      <emlib>$PROJ_DIR$\..\..\..\..\..\platform\emlib</emlib>
      <drivers>$PROJ_DIR$\..\..\..\..\..\hardware\kit\common\drivers</drivers>
      <bsp>$PROJ_DIR$\..\..\..\..\..\hardware\kit\common\bsp</bsp>
      <platform>$PROJ_DIR$\..\..\..\..\..\platform</platform>
      <kitconfig>$PROJ_DIR$\..\..\..\..\..\hardware\kit\SLSTK3701A_EFM32GG11\config</kitconfig>
      <dsp>$PROJ_DIR$\..\..\..\..\..\util\third_party\cmsis_dsp</dsp>
    </directories>
    <includepaths>
      <path>##em-path-cmsis##\Core\Include</path>
      <path>##em-path-platform##\common\inc</path>
      <path>##em-path-device##\EFM32GG11B\Include</path>
      <path>##em-path-emlib##\inc</path>
      <path>##em-path-kitconfig##</path>
      <path>##em-path-bsp##</path>
      <path>##em-path-drivers##</path>
      <path>##em-path-dsp##\DSP\Include</path>
      
    </includepaths>
    <group name="CMSIS">
      <source>##em-path-device##\EFM32GG11B\Source\$IDE$\startup_efm32gg11b.s</source>
      <source>##em-path-device##\EFM32GG11B\Source\system_efm32gg11b.c</source>
    </group>
    <group name="emlib">
      <source>##em-path-emlib##\src\em_system.c</source>
      <source>##em-path-emlib##\src\em_core.c</source>
      <source>##em-path-emlib##\src\em_emu.c</source>
      <source>##em-path-emlib##\src\em_cmu.c</source>
      <source>##em-path-emlib##\src\em_adc.c</source>
      <source>##em-path-emlib##\src\em_gpio.c</source>
    </group>
    <group name="Source">
      <source>$PROJ_DIR$\..\src\main.c</source>
      <source>$PROJ_DIR$\..\readme.txt</source>
    </group>
    <libs>
      <lib except_ide="iar">CMSISDSPTransform</lib>
      <lib except_ide="iar">CMSISDSPComplexMath</lib>
      <lib except_ide="iar">CMSISDSPCommon</lib>
      <lib except_ide="iar">m</lib>
    </libs>
    <cflags>
      <define>RETARGET_VCOM</define>
      <define>__FPU_PRESENT</define>
      <flag only_ide="slsproj">-Wp,-w</flag>
      <define>ARM_MATH_CM4</define>
      <tooloption only_ide="slsproj">  &lt;includePath uri="$(sdkInstallationPath:default())/util/third_party/cmsis_dsp/DSP/Include"/&gt;</tooloption>
      <tooloption only_ide="slsproj">  &lt;libraryPath uri="$(sdkInstallationPath:default())/util/third_party/cmsis_dsp/libs/GCC/cortex-m4"/&gt;</tooloption>
    </cflags>
  </project>
</workspace>
